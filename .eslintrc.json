{
  "env": {
    "browser": true,
    "es2021" : 2021
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "js": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-no-undef": "off",
    "react/jsx-filename-extension": "off",
    "no-undef": "off",
    "linebreak-style": 0
  }
}
